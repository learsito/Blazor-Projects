<div class="DoorBox" >
    <a href="@ProjectPage">
        <img src="../images/@Image" alt="@Title" class="ProjDoorImg">
        <h3>@Title</h3>
        <p>@Description</p>
    </a>
</div>

@code {
    [Parameter]
    public string? ProjectPage { get; set; }
    [Parameter]
    public string? Image { get; set; }
    [Parameter]
    public string? Title { get; set; }
    [Parameter]
    public string? Description { get; set; }


}